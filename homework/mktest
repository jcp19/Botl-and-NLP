#!/usr/bin/perl

use strict;
use utf8::all;

my @word;
my @translation;
my @imageURL;
my @wordAudioUrl;
my @translationAudioUrl;
my @wordTransliteration;
my $id = 0;

my $string = join("", <>);

while ($string =~ /\"(.*?)\":\"(.*?)\":\"(.*?)\":\"(.*?)\":\"(.*?)\":\"(.*?)\"/g) {
    push(@word, $1);
    push(@translation, $2);
    push(@imageURL, $3);
    push(@wordAudioUrl, $4);
    push(@translationAudioUrl, $5);
    push(@wordTransliteration, $6);
}
my %funcoes = lertemplates();
# my $linhas = join ("", map {aplicafuncao("linha",
#     en => $pen{$_},
#     pl => $ppl{$_}
#     )} 0..(@pen-1));
#my $linhas;

#mudar isto para gerar a lsta do verificador
my $var = "[";
for (0..(@word-1)) {
    $var .= "{\"id\":$id, \"word\":\"@word[$id]\",\"translation\":\"@translation[$id]\",\"imageUrl\":\"@imageURL[$id]\",\"wordAudioUrl\":\"@wordAudioUrl[$id]\",\"translationAudioUrl\":\"@translationAudioUrl[$id]\",\"wordTransliteration\":\"\",\"translationTransliteration\":\"\",\"hits\":0,\"misses\":0},";
    $id = $id + 1;
}
$var .= "]";

my $col1;
for (0..5){
  $col1 .= coluna1( inc => $_ , palpt => $word[$_]);
}
my $col2;
for (0..5){
  $col2 .= coluna2( inc2 => $_, id => $_, pal_ph => $translation[$_]);
}

# print $linhas;

# comeca aqui
my $result = mkexer(
  dict => $var,
  coluna1 => $col1,
  coluna2 => $col2
);

#
print $result;
#
## ---------------------
sub aplicafuncao {
    my $nome = shift;
    my %params = @_;
    my $result = $funcoes{$nome};
    for (keys %params) {
       $result =~ s/§$_\b/$params{$_}/g;
    }
    return $result;
}

sub lertemplates {
    my $text = join("", <DATA>);
    my %funcoes;
    while ($text =~ /__(\w+)__(.*?)(?=\n__|$)/gs) {
        $funcoes{$1} = $2;
        eval "sub $1 {aplicafuncao('$1', \@_)}";
    }
    return %funcoes;
}

__DATA__
__mkexer__
